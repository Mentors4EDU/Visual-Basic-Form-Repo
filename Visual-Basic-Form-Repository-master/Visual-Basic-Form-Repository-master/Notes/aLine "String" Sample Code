    Private Sub btnJoin_Click(sender As Object, e As EventArgs) Handles btnJoin.Click
        Dim etc() As String = {"hello", "how", "are", "you"}
        Dim nums() As Integer = {1, 2, 3, 4}
        Dim aLine As String
        For Each x In etc
            lstOut.Items.Add(x)
        Next
        aLine = Join(etc, "-")
        lstOut.Items.Add(aLine)
        aLine = Join(etc, "-")
        lstOut.Items.Add(aLine)
    End Sub
