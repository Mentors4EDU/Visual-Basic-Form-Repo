Private Sub btnPage73_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnPage73.Click
    Dim MyStr As String = "         Hello its me    "
    Dim ShortString As String = ""

    'output length property of myStr and shortstring
    lstOut.Items.Add(Mystr.Length)
    lstOut.Items.Add(ShortString.Length)
    'output upper case of myStr
    lstOut.Items.Add(Mystr.ToUpper)
    'output lower case of myStr
    lstOut.Items.Add(Mystr.ToLower)
    'use trim method to remove extra spaces of myStr and write to ShortString
    ShortString = Mystr.Trim
    'output ShortString
    lstOut.Items.Add(ShortString)
    'output the first character of ShortString
    lstOut.Items.Add(ShortString.Substring(0.1))
    'output the last chacter of shortstring
    lstOut.Items.Add(ShortString.Substring(ShortString.Length - 1, 1))
    'find position of  "me" in ShortString
    lstOut.Items.Add(ShortString.IndexOf("me"))
    'find position of  "x" in ShortString
    lstOut.Items.Add("The position of x in shorstring" & Shortstring.IndexOf "X and y".)


End Sub

