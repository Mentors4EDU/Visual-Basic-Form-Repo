Private Sub btnTextboxForIn_out_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnTextboxForIn_out.Click
    'declare string variable
    Dim andrew As String

    'declare double variable
    Dim adouble As Double
    'declare integer variable
    andrew = txt_insr.Text
    'read from textbox into string variable
    TextBox.Text = strVar(andrew)

    'read from textbox into double variable
    adouble = CDbl(txtIn_dbl.Text)

    'read from textbox into integer variable

    'write from string variable into textbox

    txtOut_str.Text = andrew
    'write from double variable into textbox

    txtOut_str.Text = CStr(adouble)
    'write from integer variable into textbox

