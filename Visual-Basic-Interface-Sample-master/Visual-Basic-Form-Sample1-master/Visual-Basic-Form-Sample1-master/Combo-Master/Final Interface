Public Class Form1

    Private Sub btnTry_Click(sender As Object, e As EventArgs) Handles btnTry.Click
        Dim numIn As Integer
        Try
            numIn = CInt(txtTry.Text)
        Catch
            MessageBox.Show("Enter a valid number in the textbox")
            numIn = 0
        End Try
        lstout.items.add(numIn)
    End Sub

    Private Sub btnData_Click(sender As Object, e As EventArgs) Handles btnData.Click
        Dim stuff() As String = {"a", "b", "c", "d"}
        lstOut.DataSource = stuff
        'The source of data to list out the listbox
    End Sub

    Private Sub TextBox1_TextChanged(sender As Object, e As EventArgs) Handles txtNum.TextChanged

    End Sub

    Private Sub btnDataS_Click(sender As Object, e As EventArgs) Handles btnDataS.Click
        Dim IndexPos As Integer
        IndexPos = CInt(txtNum.Text)
        If IndexPos < lstOut.Items.Count Then
            lstOut.Items.RemoveAt(IndexPos)
        End If
    End Sub

    Private Sub btnRemove_Click(sender As Object, e As EventArgs) Handles btnRemove.Click
        Dim stringIn As String
        stringIn = txtString.Text
        MessageBox.Show(CStr(lstOut.Items.IndexOf(stringIn)))
        Do While lstOut.Items.IndexOf(stringIn) <> -1
            lstOut.Items.Remove(stringIn)
        Loop
    End Sub

